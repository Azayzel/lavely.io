  "scripts": {
    "dev": "concurrently -r -k \"yarn start:server\" \"yarn start:client\"",
    "start:server": "nodemon --watch ./server --exec yarn pre-dev:server",
    "pre-dev:server": "yarn build:server && yarn serve",
    "serve": "node build/server.js",
    "build": "yarn build:client && yarn build:server",
    "start:client": "react-scripts start",
    "build:client": "react-scripts build",
    "build:server": "babel --copy-files ./server --out-dir ./build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
